{
  "name": "@valotas/web",
  "version": "2.2.12",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/valotas/valotas.com.git"
  },
  "private": true,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine-core": "^3.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^2.7.1",
    "serve": "^13.0.2",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint \"src/**/*.+(ts|js|tsx|jsx)\"",
    "fix": "npm run lint -- --fix",
    "build": "parcel build src/robots.txt --no-autoinstall",
    "deploy": "npm run build -- --reporter @valotas/parcel-reporter-staticsite",
    "watch:build": "parcel watch src/robots.txt --no-autoinstall",
    "serve": "serve dist",
    "preversion": "npm run lint",
    "version": "npm run deploy",
    "postversion": "git add . && git commit --amend --no-edit"
  },
  "dependencies": {
    "parcel": "^2.3.1"
  }
}
