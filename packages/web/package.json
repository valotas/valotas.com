{
  "name": "@valotas/web",
  "version": "3.13.1",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/valotas/valotas.com.git"
  },
  "private": true,
  "devDependencies": {
    "@parcel/optimizer-cssnano": "^2.12.0",
    "postcss": "^8.4.39",
    "postcss-import": "^16.1.0",
    "postcss-url": "^10.1.3",
