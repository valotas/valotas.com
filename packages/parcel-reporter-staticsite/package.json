{
  "name": "@valotas/parcel-reporter-staticsite",
  "version": "3.11.0",
  "main": "dist/StaticSiteReporter.js",
  "engines": {
    "parcel": "2.x"
  },
  "devDependencies": {
    "rimraf": "^5.0.7"
  },
  "scripts": {
    "clean": "rimraf dist node_modules",
    "lint": "eslint \"src/**/*.+(ts|js|tsx|jsx)\"",
