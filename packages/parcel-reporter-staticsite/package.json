  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.54.1",
    "@aws-sdk/client-s3": "^3.53.1",
    "@types/mime-types": "^2.1.1",
    "chalk": "^4.1.2",
    "mime-types": "^2.1.34"
  }
}
