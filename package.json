    "@types/node": "^6.0.46",
    "@types/pug": "^2.0.3",
    "@types/through2": "^2.0.32",
    "@types/vinyl-fs": "^0.0.28",
    "browser-sync": "2.17.5",
    "eslint": "^3.12.2",
    "gulp": "3.9.1",
    "gulp-cssmin": "0.1.7",
    "gulp-html-replace": "1.6.1",
    "gulp-htmlmin": "3.0.0",
    "gulp-if": "2.0.1",
    "gulp-inject": "4.1.0",
    "gulp-jasmine": "2.4.2",
    "gulp-rename": "1.2.2",
    "gulp-rev": "7.1.2",
    "gulp-sass": "2.3.2",
    "gulp-util": "3.0.7",
    "jasmine-core": "2.5.2",
    "node-fetch": "1.6.3",
    "pug": "^2.0.0-beta6",
    "rimraf": "2.5.4",
    "systemjs-builder": "0.15.34",
    "through2": "2.0.1",
    "tslib": "^1.5.0",
    "tslint": "^4.2.0",
    "typescript": "^2.2.1",
    "typings": "^2.0.0",
    "vinyl": "2.0.0",
    "vinyl-fs": "2.4.4",
    "yargs": "^7.0.2"
  },
  "scripts": {
    "postinstall": "typings --version && typings install && npm rebuild node-sass",
    "tsc": "tsc --version && tsc -p . -w",
    "build": "gulp build",
    "gulp": "gulp",
    "lint": "eslint '**/*.js' && tslint 'src/**/*.{ts,tsx}'",
    "test": "gulp tsc && gulp test",
    "rsync": "rsync -azP dist/ valotas@pi:~/valotas.com",
    "aws:sync": "aws s3 sync dist s3://valotas.com",
    "deploy": "npm version patch -m \"Release v%s\"",
    "preversion": "npm run lint && npm test",
    "postversion": "gulp dist && npm run rsync && npm run aws:sync && git push origin master --tags",
    "start": "gulp play"
  },
  "dependencies": {
    "base64-js": "0.0.8",
    "font-awesome": "^4.2.0",
    "foundation-sites": "^6.2.1",
    "marked": "0.3.5",
    "moment": "2.13.0",
    "preact": "^7.2.0",
    "preact-render-to-string": "^3.6.0",
    "systemjs": "0.19.41"
  }
